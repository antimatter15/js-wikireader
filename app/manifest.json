{
  "name": "Offline Wiki",
  "description": "An offline wikipedia reader.",
  "version": "1.0.1",
  "app": {
    "launch": {
      "local_path": "v2.html"
    }
  },
  "icons": {
    "16": "image/16.png",
    "64": "image/64.png",
    "128": "image/128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "http://commondatastorage.googleapis.com",
    "http://*/"
  ]
}
